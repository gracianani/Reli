@model ReliDemo.ViewModels.HeatIndexHistoryViewModel

@{
    ViewBag.Title = "Customers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row-fluid">
<div class="span8">
<p>
    @Html.ActionLink("返回热力站", "StationRealTime", new  { stationId = @Model.StationId }, new { @class = "btn btn-primary"})
</p>
</div>
</div>
<div class="row-fluid clearfix stationTitle">
    <h1>热力站名称</h1>
</div>

<table class="table  table-bordered table-hover  table-condensed table-clickhighlight datatable" style="background:white">
    <thead><tr>
		<th>时间</th>
		<th>热指标(Kcal/h*㎡)</th>
		<th>修改人</th>
    </tr></thead>
	<tbody>
        @foreach (var heatIndexAudit in Model.HeatIndexes)
        {
        <tr>
		    <td> @heatIndexAudit.UpdatedAt.ToString("yyyy-MM-dd HH:mm:ss")</td>
		    <td> @heatIndexAudit.参考热指标</td>
		    <td> @heatIndexAudit.User.FullName </td>
        </tr>
        }
	</tbody>

</table>

